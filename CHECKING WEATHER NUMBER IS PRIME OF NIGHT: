n1 = int(input("Enter the number n1: "))

if n1 < 2:
    print(n1, "is not a prime number")
else:
    for i in range(2, n1):
        if n1 % i == 0:
            print(n1, "is not a prime number")  # Corrected output message
            break
    else:
        print(n1, "is a prime number")  # Only executes if no divisor is found
